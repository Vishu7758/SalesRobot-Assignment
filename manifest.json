{
  "manifest_version": 3,
  "name": "LinkedIn Autoconnect",
  "version": "1.0",
  "description": "Automatically send connection requests on LinkedIn",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://*.linkedin.com/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
 "host_permissions": ["*://*/*"]
}
